# Production Environment Variables for Letsee

# Database
POSTGRES_USER=letsee_user
POSTGRES_PASSWORD=letsee_password_change_me_in_production
POSTGRES_DB=letsee

# Domain
DOMAIN=letsee.example.com

# Secret Key (generate with: python -c "import secrets; print(secrets.token_urlsafe(32))")
SECRET_KEY=your-super-secret-key-change-this-in-production

# MinIO S3 Storage
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin_change_me_in_production
MINIO_BUCKET=letsee-uploads

# Let's Encrypt Email
ACME_EMAIL=admin@letsee.example.com
